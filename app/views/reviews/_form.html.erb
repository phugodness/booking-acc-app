<%= form_for([@room, @room.reviews.build]) do |f| %>
  <p>
    <%= f.label :rank %><br>
    <%= f.select :rank, (1...10).collect{|u| [u,u]} %>
  </p>
  <p>
    <%= f.label :comment %><br>
    <%= f.cktext_area :comment %>
  </p>
  <p>
    <%= f.hidden_field :user_id, value: current_user.id%>
    <%= f.submit %>
  </p>
<% end %>
